{
  "name": "videoengager-aws-connect",
  "version": "1.0.0",
  "description": "This project contains source code and supporting files for a serverless application that you can deploy with the SAM CLI. It includes the following files and folders.",
  "scripts": {
    "test": "jest",
    "test:frontend": "jest --selectProjects=Frontend",
    "test:node": "jest --selectProjects=Backend",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint src/ --ext .js,.jsx,.mjs",
    "lint:fix": "eslint src/ --ext .js,.jsx,.mjs --fix",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "validate": "npm run lint && npm run test:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VideoEngager/aws-marketplace-subscription-app.git"
  },
  "keywords": [
    "videoengager",
    "aws",
    "connect"
  ],
  "author": "VideoEngager",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VideoEngager/aws-marketplace-subscription-app/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "homepage": "https://github.com/VideoEngager/aws-marketplace-subscription-app#readme",
  "dependencies": {
    "@babel/preset-env": "^7.28.0"
  }
}
